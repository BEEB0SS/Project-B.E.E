    </link>

    <joint name="right_front_bar_joint" type= "fixed">
        <parent link="right_middle_balancer"/>
        <child link="right_front_bar"/>
        <origin xyz="-0.1397 0 0" rpy="0 ${5*pi/4} 0"/>
